{
    "repositories": [
        {
            "type": "path",
            "url": "packages/phpcfdi/csf-pdf-scraper"
        }
    ],
    "name": "phpcfdi/csf-pdf-scraper",
    "license": "MIT",
    "authors": [
        {
            "name": "Cesar Aguilera",
            "email": "cesargnu29@gmail.com"
        }
    ],
    "require": {
        "guzzlehttp/guzzle": "^7.0",
        "phpcfdi/image-captcha-resolver": "^0.2.1",
        "symfony/dom-crawler": "^6.0",
        "symfony/css-selector": "^6.0",
        "symfony/panther": "^2.0",
        "vlucas/phpdotenv": "^5.6"
    },
    "require-dev": {
        "dbrekelmans/bdi": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "PhpCfdi\\CsfPdfScraper\\": "src/"
        }
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
